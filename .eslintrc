{
  "parser": "babel-eslint",
  "extends": ["standard", "standard-trailing-commas", "standard-react"],
  "globals": {
    "graphql": true,
    "__PATH_PREFIX__": true
  },
  "rules": {
    "react/prop-types": "off",
    "camelcase": "off",
    "allowTemplateLiterals": true,
    "space-before-function-paren": ["error", "never"]
  }
}
